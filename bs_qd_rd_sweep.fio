directory=/media/usbstick # <-- change to a safe test device or a pre-created file
size=8G
time_based=1
runtime=20                # <-- choose 45 for Agilexâ„¢ 5 E-series 
ramp_time=5
numjobs=1
group_reporting
thread=1
cpus_allowed=2,3

