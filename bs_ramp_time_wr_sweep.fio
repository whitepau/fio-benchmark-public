[global]
ioengine=io_uring                     # or: libaio
direct=1
filename=/media/usbstick/fio_testfile # <-- change to a safe test device or a pre-created file
size=16G
time_based=1
runtime=8s
numjobs=1
cpus_allowed=2,3                      # <-- force A76 cores on Agilexâ„¢ 5 SoC
end_fsync=1

iodepth=128
rw=write

